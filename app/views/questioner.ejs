<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Halaman Questioner</h1>
    <a href="/questioners/add">Add Questioner</a>
    <a href="/">Back to Home</a>
    <table border="1"="width:25%">
      <thead>
        <tr>
          <th>No</th>
          <th>Questioner</th>
          <th>Dimensi</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% var i=1; dataQuestioner.forEach(data =>{ %>
          <tr>
            <td><%=i++%></td>
            <td><%=data.pertanyaan%></td>
            <td><%=data.dimensi%></td>
            <td>
              <a href="questioners/edit/<%=data.id%>">Edit</a>
              <a href="questioners/delete/<%=data.id%>">Delete</a>
            </td>
          </tr>
        <%})%>
      </tbody>
   </table>
  </body>
</html>
