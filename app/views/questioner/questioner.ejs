<!DOCTYPE html>
<html>
  <head>
<%- include ../partial/head%>
  </head>
  <body>
      <%- include ../partial/menubar%>
    <h1>Halaman Questioner</h1>
    <!-- <div class="padding-border"> -->
      <div class="container">
        <div class="table-responsive" class="well well-sm">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>No</th>
                <th>Questioner</th>
                <th>Dimensi</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              <% var i=1; dataQuestioner.forEach(data =>{ %>
                <tr>
                  <td>
                    <%=i++%>
                  </td>
                  <td>
                    <%=data.pertanyaan%>
                  </td>
                  <td>
                    <%=data.dimensi%>
                  </td>
                  <td>
                    <a href="questioners/edit/<%=data.id%>">Edit</a>
                    <a href="questioners/delete/<%=data.id%>">Delete</a>
                  </td>
                </tr>
                <%})%>
            </tbody>
          </table>
        </div>
      </div>
    <!-- </div> -->
  </body>
</html>
